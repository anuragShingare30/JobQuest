generator client {
  provider = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]  
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Jobs {
  id Int @id @default(autoincrement())
  clerkId String 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  position String
  company String
  location String
  status String
  mode String
}